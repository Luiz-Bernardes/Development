connection
	$ ssh -p 60333 user@ip
		> p = port
		> user, ex: root
		> ip, ex: 177.40.120.60

directories
	$ cd opt/nginx/html
		~ projects
		~ current

-------------------------------

start/stop/restart - nginx
	$ /etc/init.d/nginx stop
	$ /etc/init.d/nginx start
	$ sudo service nginx restart

start/stop/restart - cron
	$ /etc/init.d/cron stop
	$ /etc/init.d/cron start
	$ sudo service cron restart	